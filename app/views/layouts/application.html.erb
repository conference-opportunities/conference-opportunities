<!DOCTYPE html>
<html lang="<%= I18n.locale %>">
<head>
  <title><%= t('.title') %></title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body class="<%= controller_name %> <%= action_name %>">
<%= render 'application/header' %>
<%= render 'flash', flash: flash %>
<main role="main">
<%= yield %>
</main>
</body>
</html>
